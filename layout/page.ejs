<link rel="stylesheet" type="text/css" href="<%= url_for('css/sora-content.css')%>" />
<link href="https://cdn.bootcss.com/social-share.js/1.0.16/css/share.min.css" rel="stylesheet">
<script src="https://cdn.bootcss.com/social-share.js/1.0.16/js/social-share.min.js"></script>
<%- partial('_partial/screen', {'is_content_screen': true}) %>
<div id="sora-content">
    <div class="scrollbar" id="sora-topbar"></div>
    <%- partial('_partial/navigator')%>
    <main class="main-font">
        <article>
            <%- page.content %>
        </article>
        <div class="social-share"></div>
        <% if (page.comments) {%>
        <div id="disqus_thread"></div>
        <script>
            (function() { // DON'T EDIT BELOW THIS LINE
                var d = document, s = d.createElement('script');
                s.src = 'https://kipsora.disqus.com/embed.js';
                s.setAttribute('data-timestamp', +new Date());
                (d.head || d.body).appendChild(s);
            })();
        </script>
        <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
        <% } %>
        <hr />
        <%- partial('_partial/footer')%>
    </main>
    <button id="sora-totop" class="button-totop">
        <i class="fa fa-angle-double-up" aria-hidden="true"></i>
    </button>
</div>
<script src="<%= url_for('js/sora-content.js') %>"></script>
<script src="<%= url_for('js/sora.js') %>"></script>
